<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.evideostb.training.chenhuan.mediaplayer">

    <uses-permission android:name="android.permission.RECORD_AUDIO"/>

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
        <!--<activity android:name=".recyclerview_demo.RecyclerViewActivity">-->
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".videomedia_demo.VideoMediaActivity" />
        <activity android:name=".soundplay_demo.SoundPoolActivity" />
        <activity android:name=".recyclerview_demo.RecyclerViewActivity" />
        <activity android:name=".audiorecorder_demo.AudioRecorderActivity" />
        <activity android:name=".glsufaceview_demo.GLSufaceViewActivity" />
    </application>

</manifest>